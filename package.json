{
  "name": "purescript-next-example",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "next build",
    "clean": "rimraf .next out output",
    "dev": "next",
    "export": "next export",
    "postinstall": "bower install",
    "serve": "serve -p 5000 out",
    "start": "next start",
    "test": "pulp test",
    "test:watch": "pulp --watch test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run lint-staged",
      "post-commit": "git update-index --again"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "next": "^8.1.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "bower": "^1.8.8",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "prettier": "^1.17.0",
    "pulp": "^12.4.0",
    "purescript": "^0.12.5",
    "purescript-psa": "^0.7.3",
    "purs-loader": "^3.4.0",
    "rimraf": "^2.6.3",
    "serve": "^11.0.0"
  }
}
